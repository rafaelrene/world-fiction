{
  "name": "remix-template-netlify",
  "private": true,
  "description": "",
  "license": "",
  "sideEffects": false,
  "scripts": {
    "generate:css": "npx tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css",
